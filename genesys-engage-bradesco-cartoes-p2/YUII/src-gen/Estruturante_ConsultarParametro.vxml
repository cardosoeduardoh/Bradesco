	<!-- This is the Assign block -->
	<form id="ConsultarParametro_Parse">
		<block>
			<script>
				try {
					
						var response = AppState.objResponseAPI;
					
						__Log('###### Log API Consultar Parametros - response: ' + JSON.stringify(response) + '######');
					
						for (var i = 0; i&lt; response.result.length; i++) {
					    	var linha = response.result[i];
					
							//TRATAMENTO V0 - INICIO
					    	if (linha.codigoParametro == &quot;Banner1&quot; || linha.codigoParametro == &quot;Banner2&quot;) {
					    		AppState.io_dados['parametros'][linha.codigoParametro] = {}
					    		AppState.io_dados['parametros'][linha.codigoParametro]['conteudo'] = linha.conteudo;
					      		AppState.io_dados['parametros'][linha.codigoParametro]['situacao'] = linha.situacao;
					      		
					      		//tratamento CORTA FRASE
					      		if (linha.hasOwnProperty(&quot;atributos&quot;)) {
					      			if (linha.atributos.hasOwnProperty(&quot;cortaFrase&quot;)) {
					    				AppState.io_dados['parametros'][linha.codigoParametro]['cortaFrase'] = linha.atributos.cortaFrase;
					    			} else {
					    				AppState.io_dados['parametros'][linha.codigoParametro]['cortaFrase'] = 'N';
					    			}
					    		} else {
					    			AppState.io_dados['parametros'][linha.codigoParametro]['cortaFrase'] = 'N';
					    		}
					    	} else {
					    		if (linha.situacao == 2) { // parametro ativo 
					    			AppState.io_dados['parametros'][linha.codigoParametro] = linha.conteudo;
					    	    } else {
					    			AppState.io_dados['parametros'][linha.codigoParametro] = '';
					    		}
					    	}
					    	//TRATAMENTO V0 - FIM
					
					    	//TRATAMENTO V2 - INICIO
					    	AppState.io_dados['parametrosV2'][linha.codigoParametro] = linha;	    	
					    	//TRATAMENTO V2 - FIM
						}
						
						AppState.io_dados.parametros = trataParametros(AppState.io_dados);
					
						AppState.io_dados.Estruturante['ConsultarParametro'] = {}
						AppState.io_dados.Estruturante['ConsultarParametro']['codigoRetorno'] = response.detail.codigoRetorno;
						AppState.io_dados.Estruturante['ConsultarParametro']['msgRetorno'] = response.detail.msgRetorno;
					
						__Log('===============================================================================================================');
						__Log('########### AppState.io_dados.parametros: ' + JSON.stringify(AppState.io_dados.parametros));
						__Log('########### AppState.io_dados.parametrosV2: ' + JSON.stringify(AppState.io_dados.parametrosV2));
						__Log('########### AppState.io_dados.Estruturante.ConsultarParametro: ' + JSON.stringify(AppState.io_dados.Estruturante.ConsultarParametro));
						__Log('===============================================================================================================');
					
					} catch (error) {
						__Log(&quot;##### Error(Log API Consultar Parametros): &quot; + error);
						AppState.io_dados.Estruturante['ConsultarParametro'] = {}
						AppState.io_dados.Estruturante['ConsultarParametro']['codigoRetorno'] = '99';
						AppState.io_dados.Estruturante['ConsultarParametro']['msgRetorno'] = JSON.stringify(error);
					}
			</script>	
			<goto next="#VerRetornoWR" />
		</block>
	</form>

