	<!-- This is the Assign block -->
	<form id="ECMA_PreparaLog">
		<block>
			<script>
				try {
					
						var response = AppState.objResponseAPI;
						var tamanho = response.result.length;
						var quantidadePacotes = 0;
						var indiceAtual = 0;
						var tamanhoPacote = 30;
					
						if (tamanho &gt;= tamanhoPacote) {
							quantidadePacotes = parseInt((tamanho / tamanhoPacote),10);
							quantidadePacotes += (parseInt((tamanho % tamanhoPacote),10) &gt; 0) ? 1 : 0;
						} else {
							quantidadePacotes = 1;
						}
					
						
						__Log('#### tamanho: ' + tamanho);
						__Log('#### quantidadePacotes: ' + quantidadePacotes);
						
						for (var i=0; i&lt;quantidadePacotes; i++) {
							AppState.VAR_arrayPacotesLog[i] = [];
						}
						
						__Log(&quot;##### VAR_arrayPacotesLog: &quot; + JSON.stringify(AppState.VAR_arrayPacotesLog));
					
						for (var i = 0; i &lt; tamanho; i++) {
							if (i &gt;0 &amp;&amp; i%tamanhoPacote == 0) {
								indiceAtual += 1;
							}
							AppState.VAR_arrayPacotesLog[indiceAtual].push(response.result[i]);
							
						}
						__Log(&quot;##### VAR_arrayPacotesLog: &quot; + JSON.stringify(AppState.VAR_arrayPacotesLog));
					} catch (error) {
						__Log(&quot;##### Error(ECMA_PreparaLog): &quot; + error);
					}
			</script>	
			<goto next="#ECMA_Set_Navegacao_Operacoes" />
		</block>
	</form>

