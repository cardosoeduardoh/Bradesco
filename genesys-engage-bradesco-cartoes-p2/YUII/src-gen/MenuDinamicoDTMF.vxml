	<!-- This is the Assign block -->
	<form id="SetaFalaNoInput">
		<block>
			<script>
				try {
					
						AppState.VAR_FraseNoInputNoMatch = [];
					
						if (AppState.in_json.fraseNoInput[AppState.VAR_TentativaAtual - 1] != undefined) {
							if (AppState.in_json.fraseNoInput[AppState.VAR_TentativaAtual - 1] instanceof Array) {
								AppState.VAR_FraseNoInputNoMatch = AppState.in_json.fraseNoInput[AppState.VAR_TentativaAtual - 1];
							} else {
								AppState.VAR_FraseNoInputNoMatch.push(AppState.in_json.fraseNoInput[AppState.VAR_TentativaAtual - 1]);
							}
						} else {
							AppState.VAR_FraseNoInputNoMatch.push({&quot;frase&quot;: AppState.VAR_Silencio});
						}
						__Log('#### VAR_FraseNoInputNoMatch: ' + JSON.stringify(AppState.VAR_FraseNoInputNoMatch));
					} catch (error) {
						__Log(&quot;##### Error(SetaFalaNoInput): &quot; + error);
					}
			</script>	
			<goto next="#VerRetryTentativaNoMatch" />
		</block>
	</form>

