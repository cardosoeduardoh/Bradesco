	<!-- This is the Branching block -->
	<form id="ValidaRetry">
		<block>
			<log gvp:level="1">
				<value expr="'######  AppState.io_dados.vRetry : ' + AppState.io_dados[&quot;vRetry&quot;]"/>
			</log>
    	</block>
		<block>
			<if cond="AppState.io_dados[&quot;vRetry&quot;]==0 || AppState.io_dados[&quot;vRetry&quot;]=='' || AppState.io_dados[&quot;vRetry&quot;]==undefined || AppState.io_dados[&quot;vRetry&quot;]=='vazio';">
				
				<!-- This is the OutLink -->
				<goto next="#SetaFrasesMenu" />
			</if>
			<goto next="#SetaFrasesMenu_retry" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="SetaFrasesMenu_retry">
		<block>
			
			<assign name="AppState.in_frases" expr="AppState.in_json['frases1']" />				
			<goto next="#ECMA_SetTimestampNavegacaoTipoVoz" />
		</block>
	</form>

