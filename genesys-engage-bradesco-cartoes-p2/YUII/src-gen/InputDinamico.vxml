	<!-- This is the Assign block -->
	<form id="ECMA_Set_NavegacaoEventosHangup">
		<block>
			<script>
				try {
					
					   	var dataAtual = getDataAtual();
					   	
					   	if (AppState.VAR_arrayAudiosVerbalizados.length &gt; 0 &amp;&amp; AppState.VAR_gravouLog == false) {
					
						AppState.objNavegacao = {
							tipoBloco: 1,
							tipoEvento: 1,
							timestampInicio: AppState.VAR_timestampInicio,
							timestampFim: dataAtual.substring(0,23).replace('T',' '), // &quot;9999-12-31 23:59:59.999&quot;
							sequencia: 1,
							dadosEvento: {
								evento: AppState.in_json['nomeMenu'],
								wavs: AppState.VAR_arrayAudiosVerbalizados
							}
						}
					
						AppState.io_dados.objSessao.inLogEvento.push(AppState.objNavegacao);
						
						__Log(&quot;##### objNavegacao: &quot;+ JSON.stringify(AppState.objNavegacao));
						
						} else {
						
						__Log(&quot;##### ArrayAudioVerbalizados vazio / Array já gravado&quot;);
						
						}
					
					
					} catch (error) {
						__Log(&quot;##### Error(ECMA_Set_NavegacaoEventosHangup): &quot; + error);
					}
			</script>	
