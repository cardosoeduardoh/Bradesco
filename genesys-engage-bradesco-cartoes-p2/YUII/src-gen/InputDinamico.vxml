				<goto next="#InputSenha" />
			</if>
			<if cond="AppState.tipoInput == 'cpf'">
				
				<!-- This is the OutLink -->
				<goto next="#InputCPF" />
			</if>
			<if cond="AppState.tipoInput == 'cartao'">
				
