						if (audio.hasOwnProperty(&quot;string&quot;)) AppState.VAR_arrayAudiosVerbalizados.push(audio['string'] );
						if (audio.hasOwnProperty(&quot;numero&quot;)) AppState.VAR_arrayAudiosVerbalizados.push(audio['numero'] );
						if (audio.hasOwnProperty(&quot;hora&quot;)) AppState.VAR_arrayAudiosVerbalizados.push(audio['hora'] );
					
						__Log(&quot;##### VAR_arrayAudiosVerbalizados: &quot;+ JSON.stringify(AppState.VAR_arrayAudiosVerbalizados));
