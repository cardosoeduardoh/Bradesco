	<form id="UR76_Parse_">
		<block>
			<script>
				try {
						// a partir deste ponto colocar no ecma
					
						var response = AppState.objResponseAPI;
						
						__Log('###### UR76 - response: ' + JSON.stringify(response) + '######');
					
						AppState.UR76 = {};
						
						var rx = {
					        //'AplControle': 2,
							'AplCodTrans': 4,
							'AplNumCartao': 16,
							'AplTipoSolicitacao': 1,
							'AplNumeroCelular': 19,
							'AplCodRetorno': 3
					    };
					
					    AppState.UR76 = {};
					
						if (response.detail.codigoRetorno == '0') {
					
							AppState.UR76 = trataRetornoMainFrame(rx, response.result.areaResposta.areaRespostaTexto); //Função está no funcoes.js
					
							//Campos adicionais
					
							AppState.UR76.codigoRetorno = response.detail.codigoRetorno;
							AppState.UR76.msgRetorno = response.detail.msgRetorno;
							AppState.UR76.timestampInicio = response.detail.timestampInicio;
							AppState.UR76.timestampFim = response.detail.timestampFim;
					
						} else {
						    AppState.UR76.AplCodRetorno = '99';
							AppState.UR76.codigoRetorno = response.detail.codigoRetorno;
							AppState.UR76.msgRetorno = response.detail.msgRetorno;
							AppState.UR76.timestampInicio = response.detail.timestampInicio;
							AppState.UR76.timestampFim = response.detail.timestampFim;
						}
						
						__Log('===============================================================================================================');
						__Log('########### AppState.UR76: ' + JSON.stringify(AppState.UR76));
						__Log('########### AppState.UR76.codigoRetorno: ' + AppState.UR76.codigoRetorno);
						__Log('########### AppState.UR76.AplCodRetorno: ' + AppState.UR76.AplCodRetorno);
						__Log('===============================================================================================================');
					} catch (error) {
						__Log(&quot;##### Error(API CREConector Mainframe 1.0): &quot; + error);
						AppState.UR76 = {};
						AppState.UR76.codigoRetorno = &quot;99&quot;;
						AppState.UR76.msgRetorno = JSON.stringify(error) ;
					}
			</script>	
			<goto next="#UR76_Parse" />
		</block>
