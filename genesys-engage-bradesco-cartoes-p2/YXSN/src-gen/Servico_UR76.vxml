					    AppState.objJsonRequest.areaEnvio.areaEnvioTxtRequisicao = TxBuffer;
					    AppState.objJsonRequest.areaEnvio.areaEnvioTamMaxResposta = 145;
						
						__Log('#### NUMERO CARTAO = ' + AppState.io_dados['sNumeroCartaoCPF'] + ' ####');
