					 	AppState.versao =  AppState.io_dados.opm.conectorversao;
					
						
					 	var TxBuffer = ''; 	
						TxBuffer    = 'UR85';
					 	TxBuffer += completaComZeros(AppState.io_dados.sNumeroCartaoCPF, 16);
					 	//TxBuffer += AppState.io_dados.NumeroSequencialUnico; 
					 	TxBuffer += AppState.io_dados.nsuSessao.substring(0, 3);
					  	 
					 	//JSON Request
						AppState.objJsonRequest = {};
						AppState.objJsonRequest.mnemonico = &quot;CONEXTRN&quot;; //FIXO
