					    AppState.objJsonRequest.areaEnvio.areaEnvioCodTrx = &quot;UR85&quot;;
					    AppState.objJsonRequest.areaEnvio.funcaoExecutada = &quot;consulta&quot;;
					    AppState.objJsonRequest.areaEnvio.tipoExecucao = 1;
					    AppState.objJsonRequest.areaEnvio.areaEnvioTamTxtRequisicao = TxBuffer.length;
					    AppState.objJsonRequest.areaEnvio.areaEnvioTxtRequisicao =TxBuffer;
					    AppState.objJsonRequest.areaEnvio.areaEnvioTamMaxResposta = 370;
						
						__Log('#### NUMERO CARTAO = ' + AppState.io_dados['sNumCartao'] + ' ####');
